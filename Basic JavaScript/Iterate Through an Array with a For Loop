/*
Last index: length-1
To loop until last element: i < arr.length
*/

// Setup
const myArr = [2, 3, 4, 5, 6];
var total = 0;

// Only change code below this line
for(let i=0; i<myArr.length; i++){
  console.log(myArr[i]);
  total+=myArr[i];
}
  console.log("The sum is "+total);

>>
2
3
4
5
6
The sum is 20
